        "manufacturerLink": {
            "de-DE": "https://friendsofshopware.com",
            "en-GB": "https://friendsofshopware.com"
        },
        "supportLink": {
            "de-DE": "https://github.com/FriendsOfShopware/FroshAbandonedCart",
            "en-GB": "https://github.com/FriendsOfShopware/FroshAbandonedCart"
